{
  "name": "lighthouse-performance-reports",
  "productName": "lighthouse-performance-reports",
  "version": "1.0.0",
  "description": "Execute multiple performance tests based on Google Lighthouse and save reports",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": "Alejandro Neri Jim√©nez",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "src/statics/app.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "app_lh_reports"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "@aternus/csv-to-xlsx": "^1.0.19",
    "chrome-launcher": "^0.15.0",
    "electron-squirrel-startup": "^1.0.0",
    "lighthouse": "^9.2.0",
    "objects-to-csv": "^1.3.6"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "electron": "16.0.5"
  }
}
